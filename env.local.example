# Copy this file to env.local and add your real API keys

# News API - Get free key from https://newsapi.org/register
NEWS_API_KEY=your_actual_newsapi_key_here

# YouTube API - Get free key from https://console.developers.google.com/
YOUTUBE_API_KEY=your_actual_youtube_api_key_here

# LinkedIn API - Get key from https://developer.linkedin.com/
LINKEDIN_API_KEY=your_actual_linkedin_api_key_here

# Wolfram API - Get key from https://developer.wolfram.com/
WOLFRAM_API_KEY=your_actual_wolfram_api_key_here

# Verbwire API - Get key from https://verbwire.com/
VERBWIRE_API_KEY=your_actual_verbwire_api_key_here

# Amazon API - Get key from https://webservices.amazon.com/
AMAZON_API_KEY=your_actual_amazon_api_key_here

# Database
DATABASE_URL=postgres://dashboard_user:dashboard_pass@localhost:5432/dashboard_db?sslmode=disable
REDIS_URL=redis://localhost:6379

# Service URLs
NEWS_SERVICE_URL=http://localhost:8001
JOBS_SERVICE_URL=http://localhost:8002
VIDEOS_SERVICE_URL=http://localhost:8003
DEALS_SERVICE_URL=http://localhost:8004
RECOMMENDATION_SERVICE_URL=http://localhost:8005
USER_SERVICE_URL=http://localhost:8006
NFT_SERVICE_URL=http://localhost:8007
